{
  "name": "ts-messaging",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "build::all": "nx run-many --target=build --exclude test",
    "test::all": "nx run-many --target=test",
    "lint::all": "nx run-many --target=lint",
    "changeset::add" : "pnpm changeset add",
    "changeset::version" : "pnpm changeset version",
    "changeset::version-snapshot" : "pnpm changeset version --snapshot snapshot",
    "publish::all" : "pnpm publish --json --recursive --access public",
    "publish::all--dry-run" : "pnpm publish --json --recursive --dry-run"
  },
  "private": true,
  "dependencies": {
    "@swc/helpers": "~0.5.0",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@changesets/cli": "^2.26.2",
    "@nx/eslint-plugin": "16.7.2",
    "@nx/jest": "16.7.2",
    "@nx/js": "16.7.2",
    "@nx/linter": "16.7.2",
    "@nx/vite": "16.7.2",
    "@swc-node/register": "^1.6.7",
    "@swc/cli": "~0.1.62",
    "@swc/core": "^1.3.78",
    "@types/node": "latest",
    "@typescript-eslint/eslint-plugin": "^5.60.1",
    "@typescript-eslint/parser": "^5.60.1",
    "@vitest/coverage-v8": "^0.34.2",
    "@vitest/ui": "~0.32.0",
    "eslint": "~8.46.0",
    "eslint-config-prettier": "8.1.0",
    "nx": "16.7.2",
    "prettier": "^2.6.2",
    "ts-node": "10.9.1",
    "typescript": "latest",
    "unplugin-swc": "^1.3.2",
    "vite": "^4.4.9",
    "vitest": "^0.34.2"
  },
  "nx": {
    "includedScripts": []
  }
}
